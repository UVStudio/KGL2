{
  "expo": {
    "name": "Nordin Keto App",
    "description": "An easy to use Ketogenic diet grocery shopping list app.",
    "slug": "nordin-keto-app",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/nordin-icon.png",
    "splash": {
      "image": "./assets/nordin-splash-green-slogan.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A6D17"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "icon": "./assets/nordin-adaptive-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/nordin-adaptive-icon.png",
        "backgroundColor": "#0A6D17"
      }
    },
    "web": {
      "favicon": "./assets/nordin-favicon.png"
    }
  }
}
